<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.workflow.general_backend.mapper.AdminMapper">
    <insert id="addAdmin" parameterType="com.workflow.general_backend.entity.Admin">
        insert into admin
            (aid,account,password,aname,nick_name,avatar)
        values (#{aid},#{account},#{password},#{aname},#{nickName},#{avatar})
    </insert>

    <select id="findAdminByAccount" parameterType="string" resultType="com.workflow.general_backend.entity.Admin">
        SELECT account,password from admin where account=#{account}
    </select>


</mapper>